            "content": "import React, { useState, useRef } from \"react\";\n\nimport PwdChangePopup from \"./PwdChangePopup\";\nimport AccountDeletePopup from \"./AccountDeletePopup\";\n\nimport homeIconBk from \"../../resource/images/nav/ico_home-bk.png\";\nimport \"../../resource/css/layout.css\";\nimport \"../../resource/css/contents.css\";\n\nconst Content = (props) => {\n  // const [tab, setTab] = useState({ myProfile: true, cmpInfo: false });\n  const [pwdChange, setPwdChange] = useState(false);\n  const [accountDelete, setAccountDelete] = useState(false);\n  const [toast, setToast] = useState(false);\n  const toastRef = useRef();\n\n  console.log(toastRef, toast);\n  const handleToastPopup = () => {\n    setToast(true);\n\n    setTimeout(() => {\n      setToast(false);\n    }, 2000);\n  };\n\n  return (\n    <>\n      {toast && (\n        <div id=\"toast\">\n          <div class=\"toast\" ref={toastRef}>\n            클립보드에 복사되었습니다.\n          </div>\n        </div>\n      )}\n      <div id=\"content\" class=\"content-section type04\">\n        {/* <!-- #include file=\"include_header.html\" --> */}\n        {/* <!-- header --> */}\n        <div>헤더</div>\n        {/* <!-- location --> */}\n        <ul class=\"location-wrap\">\n          <li>\n            <a href=\"dahboard.html\" target=\"_blank\">\n              <img src={homeIconBk} alt=\"\" />\n            </a>\n          </li>\n          <li>\n            <a href=\"myprofile.html\">프로필 설정</a>\n          </li>\n        </ul>\n        {/* <!-- //location --> */}\n\n        <div class=\"cnt-box-wrap\">\n          {/* <!-- cnt-inner --> */}\n          <div class=\"cnt-inner\">\n            <div class=\"content-box profile\">\n              {/* <!-- tabs --> */}\n              <div class=\"tab-wrap\">\n                <ul class=\"tabs\">\n                  <li class={tab.myProfile && \"on\"}>\n                    <button\n                      type=\"button\"\n                      class=\"tab-btn active\"\n                      data-tab=\"tab1\"\n                      onClick={() => {\n                        setTab({ myProfile: true, cmpInfo: false });\n                      }}\n                    >\n                      내 프로필\n                    </button>\n                  </li>\n                  <li class={tab.cmpInfo && \"on\"}>\n                    <button\n                      type=\"button\"\n                      class=\"tab-btn\"\n                      data-tab=\"tab2\"\n                      onClick={() => {\n                        setTab({ myProfile: false, cmpInfo: true });\n                      }}\n                    >\n                      회사 정보\n                    </button>\n                  </li>\n                </ul>\n                <span class=\"slider\" data-left=\"0\"></span>\n              </div>\n              {/* <!-- //tabs --> */}\n              <div class=\"tab-content\">\n                {tab.myProfile ? (\n                  <div class=\"content active\" id=\"tab1\">\n                    <h2 class=\"inp-title\">프로필 이미지</h2>\n                    <div class=\"profile-box\">\n                      <div class=\"profile-head\">\n                        <div class=\"profile\">\n                          {/* <!-- dropdown-wrap --> */}\n                          <div class=\"dropdown-wrap hover\">\n                            <button type=\"button\" class=\"btn-thum-edit\">\n                              <span>수정하기</span>\n                            </button>\n                            <div class=\"dropdown-box\">\n                              <ul>\n                                <li>\n                                  <span class=\"inp\">\n                                    <input\n                                      type=\"file\"\n                                      name=\"\"\n                                      id=\"file1\"\n                                      placeholder=\"파일 검색\"\n                                    />\n                                    <label for=\"file1\" class=\"btn-drop-menu\">\n                                      <span>사진 변경</span>\n                                    </label>\n                                  </span>\n                                </li>\n                                <li>\n                                  <button type=\"button\" class=\"btn-drop-menu\">\n                                    사진 삭제\n                                  </button>\n                                </li>\n                              </ul>\n                            </div>\n                          </div>\n                          {/* <!-- //dropdown-wrap --> */}\n                        </div>\n                      </div>\n                      <div class=\"profile-cnt\">\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">\n                            개인 코드{\" \"}\n                            <button\n                              type=\"button\"\n                              class=\"btn-copy on pop-toast\"\n                              onClick={() => {\n                                handleToastPopup();\n                              }}\n                            >\n                              <span>복사하기</span>\n                            </button>\n                          </p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"sklksl234\"\n                              disabled\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">이름</p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              placeholder=\"고객명\"\n                              value=\"홍길동\"\n                              readonly\n                            />\n                            <button type=\"button\" class=\"pos btn-edit\">\n                              <span class=\"txt hide\">수정</span>\n                            </button>\n                          </div>\n                          <p class=\"warning-txt02\">이름을 입력해주세요.</p>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">이용중인 플랜</p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"스타터\"\n                              disabled\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">휴대폰 번호</p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"tel\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"010.1234.5678\"\n                              disabled\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">비밀번호</p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"password\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"123456\"\n                              readonly\n                            />\n                            <button\n                              type=\"button\"\n                              class=\"pos btn-edit open-layer\"\n                              data-info=\"layer-password-change\"\n                              onClick={() => {\n                                setPwdChange(true);\n                              }}\n                            >\n                              <span class=\"txt hide\">수정</span>\n                            </button>\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">이메일</p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"sample@naver.com\"\n                              readonly\n                            />\n                            <button type=\"button\" class=\"pos btn-edit\">\n                              <span class=\"txt hide\">수정</span>\n                            </button>\n                          </div>\n                          <p class=\"warning-txt02\">\n                            정확한 이메일을 입력해주세요.\n                          </p>\n                          <p class=\"warning-txt02\">이미 가입된 이메일입니다.</p>\n                        </div>\n                        <div class=\"btn-group02\" style={{ marginTop: \"40px\" }}>\n                          <button\n                            type=\"button\"\n                            class=\"btn-link open-layer\"\n                            data-info=\"layer-account-delete\"\n                            onClick={() => {\n                              setAccountDelete(true);\n                            }}\n                          >\n                            <span>계정 삭제</span>\n                          </button>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                ) : (\n                  <div class=\"content active\" id=\"tab2\">\n                    <h2 class=\"inp-title\">회사 로고</h2>\n                    <div class=\"profile-box\">\n                      <div class=\"profile-head\">\n                        <div class=\"profile\"></div>\n                      </div>\n                      <div class=\"profile-cnt\">\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">회사 명</p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"오더킹\"\n                              readonly\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">\n                            회사 URL\n                            <button\n                              type=\"button\"\n                              class=\"btn-copy on pop-toast\"\n                              onClick={() => {\n                                handleToastPopup();\n                              }}\n                            >\n                              <span>복사하기</span>\n                            </button>\n                          </p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"www.sample1233.com\"\n                              readonly\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">대표자 명</p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"홍길동\"\n                              readonly\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">\n                            회사 주소\n                            <button\n                              type=\"button\"\n                              class=\"btn-copy on pop-toast\"\n                              onClick={() => {\n                                handleToastPopup();\n                              }}\n                            >\n                              <span>복사하기</span>\n                            </button>\n                          </p>\n                          <span class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              value=\"서울시 강남구 도산대로1234-56\"\n                              readonly\n                            />\n                          </span>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              placeholder=\"123호\"\n                              readonly\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">\n                            사업자 번호\n                            <button\n                              type=\"button\"\n                              class=\"btn-copy on pop-toast\"\n                              onClick={() => {\n                                handleToastPopup();\n                              }}\n                            >\n                              <span>복사하기</span>\n                            </button>\n                          </p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"tel\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"123-1566789-09876\"\n                              readonly\n                            />\n                          </div>\n                        </div>\n\n                        <div class=\"filter-box\">\n                          <p class=\"filter-title\">\n                            사업자등록증\n                            <button type=\"button\" class=\"btn-down on\">\n                              <span>다운로드</span>\n                            </button>\n                          </p>\n                          <div class=\"inp\">\n                            <input\n                              type=\"text\"\n                              name=\"\"\n                              id=\"\"\n                              value=\"sample.jpg\"\n                              readonly\n                            />\n                          </div>\n                        </div>\n                        <div class=\"btn-group02 flex-sidebox center\">\n                          <button\n                            type=\"button\"\n                            class=\"btn-link open-layer\"\n                            data-info=\"layer-company-out-confirm\"\n                          >\n                            <span>회사 나가기</span>\n                          </button>\n                          <button\n                            type=\"button\"\n                            class=\"btn btn-light-blue btn-inp-revise\"\n                          >\n                            <span>회사 정보 수정</span>\n                          </button>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                )}\n              </div>\n            </div>\n          </div>\n          {/* <!-- //cnt-inner --> */}\n        </div>\n      </div>\n      {/* 비밀번호 변경 팝업 */}\n      <PwdChangePopup pwdChange={pwdChange} setPwdChange={setPwdChange} />\n      {/* 계정 삭제 */}\n      <AccountDeletePopup\n        accountDelete={accountDelete}\n        setAccountDelete={setAccountDelete}\n      />\n    </>\n  );\n};\n\nexport default Content;\n"
        }
    ]
}